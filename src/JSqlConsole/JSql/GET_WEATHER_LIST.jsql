function jsql(obj) {    
    const city = obj.CITY;
  
    var sql = `
    SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED;
    SET NOCOUNT ON;
    
    SELECT A.TENANTID
        , A.ID
        , A.City
        , A.Date 
        , A.TemperatureC
        , A.Summary
    FROM EXAMPLE.WEATHERFORECASTS A WITH(NOLOCK)
    WHERE A.TENANTID  = @TENANTID
    `;

    if(city !== '') {
        sql += ` AND A.CITY LIKE @CITY + '%' `;
    }
    
    return sql;
}